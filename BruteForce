import itertools
import string

def brute_force_attack(target_password):
    characters = string.ascii_letters + string.digits  # Possible characters (a-z, A-Z, 0-9)
    attempts = 0

    for length in range(1, len(target_password) + 1):
        for guess in itertools.product(characters, repeat=length):
            attempts += 1
            guess = ''.join(guess)
            print(f"Trying: {guess}")
            if guess == target_password:
                print(f"Password found: {guess}")
                print(f"Total attempts: {attempts}")
                return

    print("Password not found.")

if __name__ == "__main__":
    target_password = "abc123"  # Replace with the password to simulate
    brute_force_attack(target_password)